{
  "title": "Leucosin/Register",
  "stylesheets": [
    {
      "type": "text/css",
      "href": "/Register/style.css"
    }
  ],
  "scripts": [
    {
      "type": "text/javascript",
      "src": "/Register/app.js"
    }
  ],
  "container": {
    "logo": {
      "src": "https://abs.twimg.com/sticky/illustrations/twitter-bird-white-on-blue.svg",
      "alt": "Twitter logo"
    },
    "form": {
      "heading": "Create your account",
      "fields": [
        {
          "label": "Full name",
          "type": "text",
          "id": "name",
          "name": "name",
          "placeholder": "Full name"
        },
        {
          "label": "Username",
          "type": "text",
          "id": "username",
          "name": "username",
          "placeholder": "Username"
        },
        {
          "label": "Email address",
          "type": "email",
          "id": "email",
          "name": "email",
          "placeholder": "Email address"
        },
        {
          "label": "Password",
          "type": "password",
          "id": "password",
          "name": "password",
          "placeholder": "Password"
        }
      ],
      "submit": {
        "text": "Sign up",
        "type": "submit"
      },
      "links": [
        {
          "text": "Already have an account?",
          "href": "http://127.0.0.1:5500/Sign-In/"
        }
      ]
    }
  }
}
